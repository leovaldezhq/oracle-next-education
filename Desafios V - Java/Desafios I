public class Perfil {
    public static void main (String [] args) {
        System.out.println("Olá, Luna");
        System.out.println("Tudo bem?");
    }
}

//desafio 3

public class Estudos {
    public static void main (String [] args) {
        System.out.println("A Radiação cósmica de fundo em microondas (RCFM) é um sinal eletromagnético emitido cerca de 380,000 anos depois do Big Bang.");
    }
}

//desafio 4

public class Soma {
    public static void main (String [] args) {
        System.out.println(10+5);
    }
}

